{
  "name": "nest-mailer-bullmq-intv",
  "version": "1.0.0",
  "private": true,
  "description": "Interview-ready NestJS email queue service with MongoDB (Mongoose), BullMQ, Winston logging, and rate limiting",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "lint": "eslint \"src/**/*.ts\" --fix || true",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.3.10",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/throttler": "^6.2.0",
    "bullmq": "^5.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.4.1",
    "mongoose": "^8.5.1",
    "nest-winston": "^1.9.7",
    "nodemailer": "^6.9.14",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/nodemailer": "^6.4.15",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
